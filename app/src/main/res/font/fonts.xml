<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--  Quicksand  -->
    <font
        android:font="@font/quicksand_light"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/quicksand_light"
        app:fontStyle="normal"
        app:fontWeight="300"
        tools:ignore="UnusedAttribute" />

    <font
        android:font="@font/quicksand_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/quicksand_light"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:ignore="UnusedAttribute" />

    <font
        android:font="@font/quicksand_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/quicksand_light"
        app:fontStyle="normal"
        app:fontWeight="500"
        tools:ignore="UnusedAttribute" />

    <font
        android:font="@font/quicksand_semibold"
        android:fontStyle="normal"
        android:fontWeight="600"
        app:font="@font/quicksand_light"
        app:fontStyle="normal"
        app:fontWeight="600"
        tools:ignore="UnusedAttribute" />

    <font
        android:font="@font/quicksand_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/quicksand_light"
        app:fontStyle="normal"
        app:fontWeight="700"
        tools:ignore="UnusedAttribute" />


    <!--  Fredoka  -->
    <font
        android:font="@font/fredoka_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/fredoka_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:ignore="UnusedAttribute" />

    <font
        android:font="@font/fredoka_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/fredoka_regular"
        app:fontStyle="normal"
        app:fontWeight="500"
        tools:ignore="UnusedAttribute" />

    <font
        android:font="@font/fredoka_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/fredoka_regular"
        app:fontStyle="normal"
        app:fontWeight="700"
        tools:ignore="UnusedAttribute" />


    <!--  MPLUSRounded1c  -->
    <font
        android:font="@font/mplus_rounded1c_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/mplus_rounded1c_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:ignore="UnusedAttribute" />

    <font
        android:font="@font/mplus_rounded1c_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/mplus_rounded1c_medium"
        app:fontStyle="normal"
        app:fontWeight="500"
        tools:ignore="UnusedAttribute" />

    <font
        android:font="@font/mplus_rounded1c_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/mplus_rounded1c_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        tools:ignore="UnusedAttribute" />


    <!--  SF Pro Rounded  -->
    <font
        android:font="@font/sf_pro_rounded_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/sf_pro_rounded_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:ignore="UnusedAttribute" />

    <font
        android:font="@font/sf_pro_rounded_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/sf_pro_rounded_medium"
        app:fontStyle="normal"
        app:fontWeight="500"
        tools:ignore="UnusedAttribute" />

    <font
        android:font="@font/sf_pro_rounded_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/sf_pro_rounded_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        tools:ignore="UnusedAttribute" />
</font-family>